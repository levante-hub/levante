{
  "name": "Levante",
  "version": "0.1.0",
  "description": "A friendly, private desktop chat app with AI and MCP integration",
  "main": "out/main/main.js",
  "homepage": "https://github.com/minte-community/levante",
  "author": {
    "name": "Levante Team",
    "email": "hello@levanteapp.com"
  },
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "prebuild": "electron-vite build",
    "build:main": "electron-vite build --main",
    "build:renderer": "electron-vite build --renderer",
    "preview": "electron-vite preview",
    "package": "electron-builder",
    "package:dir": "electron-builder --dir",
    "typecheck": "tsc --noEmit && tsc --noEmit --project tsconfig.preload.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.12",
    "@ai-sdk/gateway": "^1.0.18",
    "@ai-sdk/google": "^2.0.12",
    "@ai-sdk/openai": "^2.0.24",
    "@ai-sdk/openai-compatible": "^1.0.15",
    "@ai-sdk/react": "^2.0.33",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@hookform/resolvers": "^5.2.1",
    "@libsql/client": "^0.15.12",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@tailwindcss/vite": "^4.1.12",
    "ai": "^5.0.33",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "keytar": "^7.9.0",
    "lucide-react": "^0.541.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.4",
    "react-syntax-highlighter": "^15.6.3",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "sqlite3": "^5.1.7",
    "streamdown": "^1.0.12",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-stick-to-bottom": "^1.1.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@electron/notarize": "^3.0.2",
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "build": {
    "appId": "com.levante.app",
    "productName": "Levante",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "out/**/*"
    ],
    "mac": {
      "icon": "resources/icons/icon.icns",
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.inherit.plist"
    },
    "win": {
      "icon": "resources/icons/icon.ico",
      "target": "nsis"
    },
    "linux": {
      "icon": "resources/icons/icon.png",
      "target": "AppImage",
      "category": "Office"
    },
    "publish": {
      "provider": "github",
      "owner": "levante-hub",
      "repo": "levante"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.4.0"
}